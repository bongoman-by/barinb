{
  "eslint.validate": [
    "javascript",
    "typescript",
    "html"
  ],
  "eslint.options": {
    "extensions": [
      ".js",
      ".ts",
      "html"
    ]
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
  },
  "cSpell.words": [
    "barinb",
    "closeclick",
    "navs"
  ]
}
